{
  "name": "electron-example",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "estart": "set ELECTRON_DVELOP=true && electron . ",
    "package": "electron-packager . myApp --win --out ./package --overwrite --arch=x64 --app-version=0.0.1 --electron-version=4.1.4"
  },
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "antd": "^3.16.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
